{
  "apps": [
    {
      "name": "node-animal-one",
      "cwd": "./",
      "script": "app/modules/subscribe.js",
      "min_uptime": "60s",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "../pm2log/node-animal.stderr.log",
      "out_file": "../pm2log/node-animal.stdout.log",
      "pid_file": "../pm2log/node-animal.pid",
      "merge_logs": true,
      "max_restarts": 10,
      "max_memory_restart": "1024M",
      "watch": true,
      "exec_interpreter": "node",
      "exec_mode": "fork",
      "instances": 1,
      "autorestart": true,
      "vizion": true,
      "ignore_watch": [
        "node_modules"
      ],
      "env": {
        "NODE_ENV": "production",
        "REMOTE_ADDR": ""
      },
      "env_dev": {
        "NODE_ENV": "dev",
        "REMOTE_ADDR": ""
      },
      "env_test": {
        "NODE_ENV": "test",
        "REMOTE_ADDR": ""
      }
    }
  ]
}